<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:Android="http://schemas.android.com/apk/res-auto"
    package="com.longj.androids23">
    <!--权限设置-->
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PhotoBrowserActivity" />
        <activity android:name=".GestureActivity" />
        <activity android:name=".LayoutActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".GgkAndChangeLayoutActivity" />
        <activity android:name=".HightControlsActivity" />
        <activity android:name=".ChangeLayoutActivity" />
        <activity android:name=".AnimationActivity" />
        <activity android:name=".FileManagerActivity" />
        <activity android:name=".AudioAndVideoActivity" />
        <activity android:name=".Record_PlayVideoACs.JLVideoPalyActivity" />
        <activity android:name=".Record_PlayVideoACs.JLVideoRecordActivity"></activity>

    </application>

</manifest>